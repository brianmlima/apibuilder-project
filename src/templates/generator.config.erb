# Generated with <%= script_name %> at <%= Time.now.strftime("%d/%m/%Y %H:%M:%S") %>
settings:
  code.create.directories: true
code:
  <%= organization %>:
    <%= application %>:
      version: <%= version %>
      generators:
        openapi:
          target: generated/src/main/resources/static/<%= application %>
  <% if organization != "bml" %>
  bml:
  <% end %>
    validation:
      version: 0.0.1
      generators:
        bml_openapi:
          target: generated/src/main/resources/static/<%= application %>
        bml_lombok:
          target: generated/src/main/java
        bml_lombok_test:
          target: generated/src/test/java
